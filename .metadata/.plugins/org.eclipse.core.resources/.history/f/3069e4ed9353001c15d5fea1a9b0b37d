package com.revature.dto;

import java.util.Objects;

public class ChangeReimbursementStatusDTO {

	private String status;
	
	public ChangeReimbursementStatusDTO() {
		super();
	}

	public ChangeReimbursementStatusDTO(String status) {
		super();
		this.status = status;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	@Override
	public int hashCode() {
		return Objects.hash(status);
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		ChangeReimbursementStatusDTO other = (ChangeReimbursementStatusDTO) obj;
		return Objects.equals(status, other.status);
	}

	@Override
	public String toString() {
		return "ChangeReimbursementStatusDTO [status=" + status + "]";
	} 
}
